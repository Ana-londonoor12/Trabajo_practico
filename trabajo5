def fahrenheit_a_celsius(fahrenheit):
    celsius = (fahrenheit - 32) * 5 / 9
    return celsius
def main():
    fahrenheit_ingresados = float(input("Ingrese la temperatura en grados Fahrenheit: "))
    celsius_calculados = fahrenheit_a_celsius(fahrenheit_ingresados)
    print("La temperatura en grados Celsius es:", celsius_calculados)
main()

